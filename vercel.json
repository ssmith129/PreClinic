{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/$",
      "dest": "/index.html"
    },
    {
      "src": "/react/template/$",
      "dest": "/index.html"
    },
    {
      "src": "/react/template/dashboard$",
      "dest": "/index.html"
    },
    {
      "src": "/react/template/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}